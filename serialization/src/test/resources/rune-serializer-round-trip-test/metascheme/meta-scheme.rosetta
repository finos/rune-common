namespace serialization.test.metascheme

annotation rootType: <"Mark a type as a root of the rosetta model">

enum EnumType:
  A
  B
  C

type A:
  fieldA string (1..1)
    [metadata scheme]

type Root:
  [rootType]
  enumType EnumType (0..1)
    [metadata scheme]
  typeA A (0..1)
    [metadata scheme]
  enumTypeList EnumType (0..*)
    [metadata scheme]
  typeAList A (0..*)
    [metadata scheme]
